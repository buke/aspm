{
  "name": "aspm",
  "version": "0.1.0",
  "description": "Atom-Shell Package Manager",
  "main": "index.js",
  "bin": {
    "aspm": "bin/aspm"
  },
  "scripts": {
    "pretest": "npm run clean",
    "test": "mocha --reporter spec --require=coffee-script/register test/*.coffee",
    "clean": "rm -rf tmp/",
    "build": "coffee --output lib --compile src"
  },
  "engines": {
    "node": ">= 0.10.0"
  },
  "keywords": [
    "atom-shell",
    "package manager"
  ],
  "homepage": "https://github.com/bwin/aspm",
  "author": {
    "name": "Benjamin Winkler (bwin)"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/bwin/aspm.git"
  },
  "bugs": {
    "url": "https://github.com/bwin/aspm/issues"
  },
  "license": "MIT",
  "dependencies": {
    "commander": "^2.5.0",
    "queue-async": "^1.0.7",
    "semver": "^4.1.0",
    "terminal-colors": "^0.1.3"
  },
  "devDependencies": {
    "chai": "^1.10.0",
    "coffee-script": "^1.8.0",
    "mocha": "^2.0.1"
  }
}
